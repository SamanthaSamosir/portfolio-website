{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "redirects": [
    { "source": "http://samanthasamosir.com", "destination": "https://samanthasamosir.com", "permanent": true }
  ]
}